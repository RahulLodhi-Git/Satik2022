<body>
<main>
    <div class="admin-wrap">
        <div class="container">
            <div class="row admin">
                <div class="col-md-4 mx-auto admin-inner">
                    <div class="logo mb-2">
                        <a href="/">
                            <img src="/assets/logo.png"  width="150p" alt="Satik electric">
                        </a>
                    </div>
                    <h3 class="mb-4">Admin Login</h3>
                    <div class="admin-form form  ">
                        <form action="" method="post">
                          
                            <input type="email" name="email" id="email" placeholder="Enter email" required>
                            <input type="password" name="password" id="password" placeholder="Enter password " required>
                            <button type="submit " class="btn btn-primary rounded-0 px-4 py-1 my-2">Login</button>
                          </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
